        <Key ID="STR_keko_immerse_enableExShake">
            <English>Enable exShake</English>
            <German>Kameraschütteln aktivieren</German>
        </Key>
        <Key ID="STR_keko_immerse_enableExShakeTooltip">
            <English>CamShake for explosions, even distant ones!</English>
            <German>Kameraschütteln bei Explosionen, sogar entfernten!</German>
        </Key>

